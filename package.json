{
  "name": "Elmer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "./node_modules/.bin/forever start launch.json",
    "stop": "./node_modules/.bin/forever stop express; ./node_modules/.bin/forever stop cron; exit 0;",
    "restart": "./node_modules/.bin/forever restart express; ./node_modules/.bin/forever restart cron; exit 0;",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --reporter spec 'test/index.js'",
    "test-travis": "NODE_ENV=test ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec test/index.js"
  },
  "dependencies": {
    "async": "^1.5.2",
    "bigdecimal": "^0.6.1",
    "body-parser": "^1.14.2",
    "cookie-parser": "~1.0.1",
    "cron": "^1.1.0",
    "crypto": "0.0.3",
    "debug": "~0.7.4",
    "express": "~4.0.0",
    "forever": "^0.15.1",
    "gravatar": "^1.5.2",
    "jade": "~1.3.0",
    "json-bignum": "0.0.3",
    "limiter": "^1.1.0",
    "mandrill-api": "^1.0.45",
    "moment": "^2.11.0",
    "moment-timezone": "^0.5.1",
    "morgan": "~1.0.0",
    "newrelic": "^1.25.5",
    "node-fetch": "^1.5.3",
    "request": "^2.72.0",
    "request-promise": "^3.0.0",
    "require-all": "^2.0.0",
    "sha1": "^1.1.1",
    "stathat": "0.0.8",
    "static-favicon": "~1.0.0",
    "throttled-request": "^0.1.1",
    "wheniwork-unofficial": "^0.2.2"
  },
  "devDependencies": {
    "coveralls": "^2.11.11",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "istanbul": "^0.4.4",
    "mocha": "^2.4.5"
  }
}
